        "typedoc-plugin-markdown": "^2.2.14"
    },
    "devDependencies": {
        "@types/debug": "^4.1.5",
        "@types/isomorphic-fetch": "0.0.35",
        "@types/jest": "^24.0.18",
        "@types/node": "^12.7.4",
        "jest": "^24.1.0",
        "prettier": "^1.18.2",
        "ts-jest": "^24.0.0",
        "typedoc": "^0.15.3",
        "typescript": "~3.5.2"
    },
    "prettier": {
        "trailingComma": "es5",
