{
    "name": "mailslurp-client",
    "description": "Send and receive emails in code",
    "version": "7.1.1",
    "license": "MIT",
    "author": "mailslurp.com",
    "homepage": "https://www.mailslurp.com",
    "keywords": [
        "email",
        "smtp",
        "test",
        "e2e",
        "integration",
        "account",
        "receive",
        "send",
        "mailslurp"
    ],
    "main": "./dist/index.js",
    "typings": "./dist/index.d.ts",
    "repository": "https://github.com/mailslurp/mailslurp-client",
    "scripts": {
